{
    "description": "Order Fulfillment Example Application",
    "type": "project",
    "require": {
        "php": "~7.1",
        "fzaninotto/faker": "^1.6",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "pda/pheanstalk": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "^3.1",
	    "phpunit/phpunit": "~6.0",
        "benconstable/phpspec-laravel": "^3.0"
    },
    "autoload": {
        "classmap": [
            "laravel/database"
        ],
        "psr-4": {
            "App\\": "laravel/app/",
            "OrderFulfillment\\": "src/",
            "spec\\OrderFulfillment\\": "spec/"
        },
        "files": [ "laravel/app/Http/helpers.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "laravel/tests/",
            "spec\\OrderFulfillment\\": "spec/"
        },
        "files": ["spec/PhpSpec/expect.php"]
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
